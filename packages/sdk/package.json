{
  "name": "sdk",
  "version": "1.0.0",
  "private": true,
  "description": "TypeScript SDK and contract bindings for YesNo.Win",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run generate && tsup",
    "generate": "node scripts/generate-types.js",
    "clean": "rm -rf dist typechain",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "thirdweb": "^5.110.4",
    "viem": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}

